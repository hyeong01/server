  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/css/home/login.css">
  <script src="/js/home/register.js" defer></script>
  <title>Document</title>
</head>
<body>
  <div class="login-page">
    <div class="form">
      <form class="register-form">
        <input id='id' type="text" placeholder="아이디"/>
        <input id='pswd' type="password" placeholder="비밀번호"/>
        <input id='pswd-confirm' type="password" placeholder="비밀번호 확인"/>
        <input id='email' type="text" placeholder="이메일 주소"/>
        <button>회원 가입</button>
        <p class="message">이미 아이디가 있습니까? <a href="login">로그인 하기</a></p>
      </form>
    </div>
  </div>
</body>
</html>